  "scripts": {
    "add:component": "node bin/generate.js component",
    "build-sprites": "npm run clean-svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --dest=./styleguide/svg/sprite-dist ./styleguide/svg/sprite-source/*.svg",
    "clean-svgs": "svgo --enable='removeUselessStrokeAndFill' -f ./styleguide/svg/sprite-source",
    "copyright:fix": "copyright --fix --update 'src/**/*.js?(x)' 'bin/**/*.js?(x)' 'tests/**/*.js?(x)' 'scripts/**/*.js?(x)'",
    "copyright:lint": "copyright 'src/**/*.js?(x)' 'bin/**/*.js?(x)' 'tests/**/*.js?(x)' 'scripts/**/*.js?(x)'",
    "dev:build": "echo \"This command is deprecated. Please use 'npm run prod:build'.\"; sleep 5; npm run prod:build",
    "dev:watch": "watch 'npm run dev:build' src/",
    "docs:compile": "npm run styleguide:build && mobify-docs compile docs/public/latest",
    "docs:deploy": "npm run styleguide:build && mobify-docs deploy docs/public/latest -p progressive-web",
    "docs:dev": "npm-run-all -p mobify-docs:preview styleguide:silent",
    "lint": "npm run lint:js && npm run lint:sass && npm run copyright:lint",
    "lint:fix": "npm run lint:js -- --fix && npm run copyright:fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:sass": "sass-lint -c node_modules/mobify-code-style/css/.sass-lint.yml 'src/**/*.scss' -v",
    "mobify-docs:preview": "mobify-docs preview docs/public/latest",
    "prod:build": "cross-env NODE_ENV=production scripts/build.js",
    "styleguide": "node scripts/generate-scss-imports.js && styleguidist server",
    "styleguide:build": "node scripts/generate-scss-imports.js && styleguidist build && styleguide-rename",
    "styleguide:silent": "npm run styleguide >/dev/null",
    "test": "cross-env NODE_ENV=test jest",
    "test:all": "npm run lint && npm test -- --coverage",
    "test:inspect": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "npm test -- --watch"
  },
